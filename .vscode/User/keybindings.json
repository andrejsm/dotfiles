[
  {"key": "cmd+\\", "command": "workbench.action.toggleSidebarVisibility"},
  {"key": "ctrl+[Backquote]", "command": "workbench.action.terminal.focus", "when": "!terminalFocus"},
  {"key": "ctrl+[Backquote]", "command": "workbench.action.terminal.toggleTerminal", "when": "terminalFocus"},
  {"key": "cmd+right", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus"},
  {"key": "cmd+left", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus"},
  {"key": "cmd+w", "command": "workbench.action.terminal.kill", "when": "terminalFocus"},
  {"key": "cmd+shift+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
  {
    "key": "ctrl+shift+q",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "cmd+n",
    "command": "explorer.newFile"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+down",
    "command": "spaceBlockJumper.moveDown"
  },
  {
    "key": "ctrl+up",
    "command": "spaceBlockJumper.moveUp"
  },
  {
    "key": "ctrl+shift+down",
    "command": "spaceBlockJumper.selectDown"
  },
  {
    "key": "ctrl+shift+up",
    "command": "spaceBlockJumper.selectUp"
  },
  {
    "key": "shift+cmd+w",
    "command": "workbench.action.closePanel"
  },
  {
    "key": "shift+cmd+o",
    "command": "outline.focus"
  },
  {
    "key": "shift+alt+g",
    "command": "git-graph.view"
  },
  {
    "key": "cmd+shift+r",
    "command": "workbench.action.tasks.runTask",
    "args": "Start dev env"
  },
  {
    "key": "cmd+alt+shift+r",
    "command": "workbench.action.tasks.terminate",
    "args": "terminateAll"
  },
  {
    "key": "shift+alt+c",
    "command": "html-css-class-completion.cache"
  },
  {
    "key": "shift+cmd+w",
    "command": "editor.action.toggleRenderWhitespace",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+s a",
    "command": "editor.action.sortLinesAscending",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+s d",
    "command": "editor.action.sortLinesDescending",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+z",
    "command": "undo",
    "when": "editorFocus"
  },
  {
    "key": "cmd+z",
    "command": "-undo"
  },
  {
    "key": "shift+alt+t",
    "command": "workbench.view.extension.thunder-client"
  },
  {
    "key": "shift+cmd+t",
    "command": "-workbench.view.extension.thunder-client"
  }
]
